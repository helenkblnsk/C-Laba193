using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace laba1
{
class Program
{
static public double Method(double a, double b)
{
return a / b;
}
static void Main(string[] args)
{
double a = Convert.ToDouble(Console.ReadLine());
double b = Convert.ToDouble(Console.ReadLine());
if (b == 0)
{
Console.WriteLine("Error,because division by zero is impossible");
Console.ReadLine();

        }

        Console.WriteLine(Method(a, b));
    }
}
}
